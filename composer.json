{
  "name": "movingimage/vm6-api-client",
  "description": "API client library for the Video Manager 6 REST API",
  "type": "library",
  "license": "BSD 3-Clause License",
  "authors": [
    {
      "name": "Ruben Knol",
      "email": "ruben.knol@movingimage.com"
    }
  ],
  "require": {
    "php": "^5.5 || ^7.0",
    "movingimage/video-manager-meta": "^1.0",
    "guzzlehttp/guzzle": "^5.0 || ^6.0",
    "jms/serializer": "^1.0",
    "psr/log": "^1.0",
    "monolog/monolog": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^1.12",
    "phpunit/phpunit": ">=4.5"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "MovingImage\\Client\\VM6\\": "lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MovingImage\\Test\\Client\\VM6\\": "tests/"
    }
  }
}
